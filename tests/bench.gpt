set terminal pngcairo truecolor size 1400,400
set output 'result.png'

set multiplot layout 1,3

set format x "%1gk"

set title "Preprocessing"
set format y "%1gns"
set yrange [0:5000]

plot '/tmp/rmq.tsv' index 0 using ($1/1000):2 with lines title "Naive", \
     '/tmp/rmq.tsv' index 1 using ($1/1000):2 with lines title "Dense", \
     '/tmp/rmq.tsv' index 2 using ($1/1000):2 with lines title "Sparse", \
     '/tmp/rmq.tsv' index 3 using ($1/1000):2 with lines title "Hybrid"

set title "Memory"
set yrange [0:500]
set format y "%1gkw"

plot '/tmp/rmq.tsv' index 0 using ($1/1000):($4/1024) with lines title "Naive", \
     '/tmp/rmq.tsv' index 1 using ($1/1000):($4/1024) with lines title "Dense", \
     '/tmp/rmq.tsv' index 2 using ($1/1000):($4/1024) with lines title "Sparse", \
     '/tmp/rmq.tsv' index 3 using ($1/1000):($4/1024) with lines title "Hybrid"

set title "Mean query time"
unset yrange
set format y "%1gns"
set yrange [0:0.7]

plot '/tmp/rmq.tsv' index 0 using ($1/1000):3 with lines title "Naive", \
     '/tmp/rmq.tsv' index 1 using ($1/1000):3 with lines title "Dense", \
     '/tmp/rmq.tsv' index 2 using ($1/1000):3 with lines title "Sparse", \
     '/tmp/rmq.tsv' index 3 using ($1/1000):3 with lines title "Hybrid"
